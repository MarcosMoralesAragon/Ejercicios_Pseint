Algoritmo Ejercicio5
	
	Definir a,b,resultado Como Real;
	
	Escribir "Introduce el valor de a";
	Leer a;
	Escribir "Introduce el valor de b";
	Leer b;
	
	Si a=0 Entonces
		Escribir "No tiene solución";
	SiNo
		resultado <- (-(b))/a;
		Escribir "Resultado de la operación: ",resultado;
	FinSi
	
FinAlgoritmo
