Algoritmo Ejercicio1Parte3
	
	Definir num1, num2,multiplicacion Como Reales;
	
	Escribir "Introduce el primer numero a multiplicar";
	Leer num1;
	Escribir "Introduce el segundo numero a multiplicar";
	Leer num2;
	
	multiplicacion<-num1*num2;
	
	Escribir "Multiplicación:",multiplicacion;
	
FinAlgoritmo
